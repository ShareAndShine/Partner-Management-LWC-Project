<?xml version="1.0" encoding="UTF-8"?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>SampleMessageChannel</masterLabel>
    <isExposed>true</isExposed>
    <description>Lightning Message Channel for carring selected partner account from partner card publisher</description>
    
    <lightningMessageFields>
        <fieldName>selectedPartnerAccountId</fieldName>
        <description>This is the record Id of selected partner account</description>
    </lightningMessageFields>
    
    <lightningMessageFields>
        <fieldName>channelname</fieldName>
        <description>Name of the Channel</description>
    </lightningMessageFields>

    <lightningMessageFields>
        <fieldName>selectedPartnerAccountName</fieldName>
        <description>This is the name of the selected partner account</description>
    </lightningMessageFields>
    
</LightningMessageChannel>